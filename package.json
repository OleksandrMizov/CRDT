{
  "name": "patterns",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint . && prettier -c \"**/*.js\"",
    "fix": "eslint . --fix && prettier --write \"**/*.js\"",
    "types": "tsc -p tsconfig.json",
    "test": "node --test ./**/__tests__/**/*.test.js",
    "test:coverage": "node --test --experimental-test-coverage ./**/__tests__/**/*.test.js"
  },
  "author": "Timur Shemsedinov",
  "private": true,
  "devDependencies": {
    "eslint": "^9.12.0",
    "eslint-config-metarhia": "^9.1.1",
    "prettier": "^3.3.3",
    "typescript": "^5.9.3"
  }
}
